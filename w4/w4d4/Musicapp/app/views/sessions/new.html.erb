<!-- login page -->
<h1>Log in!</h1>

  <form class="" action=" <%= session_url %>" method="post">
    <%= auth_token %>

    <label for="user_email">Email</label>
    <input type="text"
           name="user[email]"
           id="user_email">

    <br>

    <label for="user_password">Password</label>
    <input type="password"
           name="user[password]"
           id="user_password">

    <input type="submit">
  </form>
